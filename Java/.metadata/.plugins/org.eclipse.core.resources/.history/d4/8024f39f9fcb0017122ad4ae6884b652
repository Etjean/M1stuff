import java.util.Random;


public class Exceptions {
	public static void main(String[] args) {
		
	}
	
	public static int divise(int a, int b) {
		return a/b;
	}
	
	public static int[] tab_aleatoire(int n) {
		int[] tab = new int[n];
		Random rd = new Random();
		for (int i=0; i<=n; i++){
			tab[i]=rd.nextInt(10);
		}
		return tab;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
