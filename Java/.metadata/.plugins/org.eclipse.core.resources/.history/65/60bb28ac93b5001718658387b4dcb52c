
public class Test {
	
	
	public static void main(String[] args) {
		Personne pers1 = new Personne("Cretin", "Gabriel");
		Personne pers2 = new Personne("Lematre", "Sophie");
		System.out.println(pers1.presentation());
		System.out.println(pers2.presentation());
		
		System.out.println(creeNPersonnes(6)[2].nom);
		
	}
	
	
	
	static Personne[] creeNPersonnes(int n) {
		Personne[] tabPers = new Personne[n];
		for (int i=1; i<n; i++) {
			tabPers[i].nom = "jacquie"; 
		}
		return tabPers;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
