import java.util.Random;


public class Exceptions {
	public static void main(String[] args) {
		System.out.println(divise(4, 5));
		int[] tab = tab_aleatoire(8);
		System.out.println(divisions_successives(53, tab));
	}
	
	public static int divise(int a, int b) {
		return a/b;
	}
	
	public static int[] tab_aleatoire(int n) {
		int[] tab = new int[n];
		Random rd = new Random();
		for (int i=0; i<n; i++){
			tab[i]=rd.nextInt(10);
		}
		return tab;
	}
	
	public static int divisions_successives(int n, int[] tab) {
		for (int i=0; i<tab.length; i++) {
			n = divise(n, tab[i]);
		}
		return tab[n];
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
