
public class Universit√© {
	//Attributs
	int nbMembres;
	Personne[] membres = new Personne[nbMembres];
	Travailleur president;
	
	
	
	
	//Methodes
	public double masseSalariale(){
		double ms = 0;
		ms += president.salaire;
		for (Personne p : membres){
			ms += p.salaire;
		}
		return ms;
	}
	
	
}
